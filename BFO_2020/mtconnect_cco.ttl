                                                                 owl:onProperty obo:RO_0000085 ;
                                                                 owl:someValuesFrom cco:CommunicationArtifactFunction
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf mtc:ManufacturingMachine ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty obo:RO_0000085 ;
                                      owl:someValuesFrom cco:CommunicationArtifactFunction
                                    ] .


###  urn://schemas.mtconnect.org/ontologies/mtconnect#Machine
mtc:Machine rdf:type owl:Class ;
            rdfs:subClassOf cco:Artifact .


###  urn://schemas.mtconnect.org/ontologies/mtconnect#ManufacturingArtifactFunction
mtc:ManufacturingArtifactFunction rdf:type owl:Class ;
                                  rdfs:subClassOf cco:ArtifactFunction .


###  urn://schemas.mtconnect.org/ontologies/mtconnect#ManufacturingMachine
mtc:ManufacturingMachine rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( mtc:Machine
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty obo:RO_0000085 ;
                                                                      owl:someValuesFrom mtc:ManufacturingArtifactFunction
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] ;
                         rdfs:subClassOf mtc:Machine ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty obo:RO_0000085 ;
                                           owl:someValuesFrom mtc:ManufacturingArtifactFunction
                                         ] .


###  urn://schemas.mtconnect.org/ontologies/mtconnect#ManufacturingMachineControlSystem
mtc:ManufacturingMachineControlSystem rdf:type owl:Class ;
                                      owl:equivalentClass [ owl:intersectionOf ( cco:ControlSystem
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty obo:RO_0000085 ;
                                                                                   owl:someValuesFrom cco:ComputingArtifactFunction
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ] ;
                                      rdfs:subClassOf cco:ControlSystem .


